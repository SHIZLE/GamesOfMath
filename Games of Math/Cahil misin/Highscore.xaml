        	<Run Text="   "/>
        	<Run Text=":"/>
        </TextBlock>
        <TextBlock Margin="10,130,266,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic">
        	<Run Text="Medium"/>
        	<Run Text="  :"/>
        </TextBlock>
        <TextBlock Margin="10,190,266,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic">
        	<Run Text="Hard"/>
        	<Run Text="  :"/>
        </TextBlock>
        <TextBlock Margin="0,250,0,0" TextWrapping="Wrap" Text="Classic Game" VerticalAlignment="Top" Height="49" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic" TextAlignment="Center"/>
        <TextBlock Margin="10,304,266,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic">
        	<Run Text="Easy "/>
        	<Run Text=":"/>
        </TextBlock>
        <TextBlock Margin="10,364,266,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic">
        	<Run Text="Medium"/>
        	<Run Text="  :"/>
        </TextBlock>
        <TextBlock Margin="10,424,266,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic">
        	<Run Text="Hard"/>
        	<Run Text=" :"/>
        </TextBlock>
        <TextBlock Margin="0,484,0,0" TextWrapping="Wrap" Text="Reverse Game" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic" TextAlignment="Center"/>
        <TextBlock Margin="10,539,266,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic">
        	<Run Text="Easy"/>
        	<Run Text=" :"/>
        </TextBlock>
