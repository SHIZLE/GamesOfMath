        <TextBlock x:Name="rg1" Margin="101,539,291,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic" Text="0" TextAlignment="Center"/>
        <TextBlock x:Name="rg2" Margin="149,599,243,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic" Text="0" TextAlignment="Center"/>
        <TextBlock x:Name="rg3" Margin="105,659,287,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic" Text="0" TextAlignment="Center"/>
    </Grid>

    <!--LayoutRoot is the root grid where all page content is placed-->


</phone:PhoneApplicationPage>