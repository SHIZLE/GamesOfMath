        	<Run Text="Hard"/>
        	<Run Text=" :"/>
        </TextBlock>
        <Border BorderThickness="1" Margin="150,0" Background="#FFE67E22" BorderBrush="#FFE67E22" Height="54" VerticalAlignment="Bottom">
            <TextBlock TextWrapping="Wrap" Text="RESET" Margin="-1" Foreground="#FF2C3E50" FontSize="36" TextAlignment="Center" Tap="TextBlock_Tap"/>
        </Border>
        <TextBlock x:Name="tf1" Margin="117.045,70,274.955,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic" Text="0" TextAlignment="Center" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False" d:LayoutRounding="Auto">
            <TextBlock.RenderTransform>
                <CompositeTransform Rotation="0.391"/>
            </TextBlock.RenderTransform>
        </TextBlock>
        <TextBlock x:Name="tf2" Margin="159,130,233,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" Foreground="#FF2C3E50" FontSize="36" FontFamily="Times New Roman" FontStyle="Italic" Text="0" TextAlignment="Center"/>
